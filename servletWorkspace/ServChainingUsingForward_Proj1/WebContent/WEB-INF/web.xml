<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ServChainingUsingForward_Proj1</display-name>
  <welcome-file-list>
    <welcome-file>product.html</welcome-file>
  </welcome-file-list>
  <servlet-mapping>
   <servlet-name>FirstServ</servlet-name>
   <url-pattern>/fs</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>FirstServ</servlet-name>
    <servlet-class>org.http.ServChainingUsingForward_App2.FirstServlet</servlet-class>
   </servlet>
  
  <servlet-mapping>
   <servlet-name>SecondServ</servlet-name>
   <url-pattern>/ss</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SecondServ</servlet-name>
    <servlet-class>org.http.ServChainingUsingForward_App2.SecondServlet</servlet-class>
  </servlet>
</web-app>